<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Basic AppCompat DayNight theme with ActionBar -->
    <style name="Theme.通知日历同步" parent="Theme.AppCompat.DayNight.DarkActionBar">
        <!-- Use md_theme_* colors from COLOR_SOURCE while keeping AppCompat -->
        <item name="colorPrimary">@color/md_theme_primary</item>
        <item name="colorPrimaryDark">@color/md_theme_primaryContainer</item>
        <item name="colorAccent">@color/md_theme_secondary</item>
        <item name="android:statusBarColor">@color/md_theme_primaryContainer</item>
        <item name="android:navigationBarColor">@android:color/transparent</item>
        <!-- Light status bar icons for light theme -->
        <item name="android:windowLightStatusBar">true</item>
    </style>
</resources>